{
  "openai_model": "gpt-4",
  "temperature": 0.1,
  "max_tokens": 2000,
  
  "github_api_url": "https://api.github.com",
  
  "max_comments_per_file": 10,
  "include_category_emoji": true,
  
  "exclude_extensions": [
    ".lock",
    ".md",
    ".txt",
    ".json",
    ".yaml",
    ".yml",
    ".gitignore",
    ".env",
    ".env.example",
    ".editorconfig",
    ".png",
    ".jpg",
    ".jpeg",
    ".gif",
    ".svg",
    ".ico",
    ".pdf",
    ".doc",
    ".docx",
    ".xls",
    ".xlsx",
    ".ppt",
    ".pptx"
  ],
  
  "exclude_paths": [
    "node_modules/",
    "venv/",
    "env/",
    ".venv/",
    ".env/",
    "dist/",
    "build/",
    "coverage/",
    ".coverage/",
    ".git/",
    ".github/",
    ".idea/",
    ".vscode/"
  ]
}
